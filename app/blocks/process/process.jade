mixin process()
	+b('section').section-process
		+e.in
			+e('h2').title ФОТОПРОЦЕСС СТРОИТЕЛЬСТВА ГАЗОПРОВОДА
			+e('h3').subtitle Процесс строительства подземного газопровода
			+b.work-photo
				+e('h2').title Разработка траншеи и подготовка трубы для укладки
				+e('ul').list
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-1-1.jpg' alt='photo-1')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-1-2.jpg' alt='photo-2')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-1-3.jpg' alt='photo-3')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-1-4.jpg' alt='photo-4')
			+b.work-photo
				+e('h2').title Укладка трубы в траншею, укладка песчанной подушки, врезка в газопровод
				+e('ul').list
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-2-1.jpg' alt='photo-1')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-2-2.jpg' alt='photo-2')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-2-3.jpg' alt='photo-3')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-2-4.jpg' alt='photo-4')
			+b.work-photo
				+e('h2').title Укладка сигнальной ленты, подводка трубы к дому и пробивка отверстия для трубы
				+e('ul').list
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-3-1.jpg' alt='photo-1')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-3-2.jpg' alt='photo-2')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-3-3.jpg' alt='photo-3')
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-3-4.jpg' alt='photo-4')
					+b.work-photo
				+e('h2').title Готовая котельная
				+e('ul').list
					+e('li').i
						+e.i-img
							+e('img')(src='assets/images/content/photo-group-4-1.jpg' alt='photo-1')

